<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model id="protein_expression" name="protein_expression" substanceUnits="mole" timeUnits="second" extentUnits="mole">
    <listOfUnitDefinitions>
      <unitDefinition id="per_second">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="default_compartment" spatialDimensions="3" size="1" units="litre" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="GFP__mRNA" compartment="default_compartment" initialConcentration="0" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="GFP__protein" compartment="default_compartment" initialConcentration="0" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="GFP__omega" value="1" units="per_second" constant="true"/>
      <parameter id="GFP__phi" value="1" units="per_second" constant="true"/>
      <parameter id="GFP__k" value="1" units="per_second" constant="true"/>
      <parameter id="GFP__d" value="1" units="per_second" constant="true"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="GFP___J0" reversible="false">
        <listOfProducts>
          <speciesReference species="GFP__mRNA" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> GFP__omega </ci>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="GFP___J1" reversible="false">
        <listOfReactants>
          <speciesReference species="GFP__mRNA" constant="true"/>
        </listOfReactants>
        <listOfModifiers>
          <modifierSpeciesReference species="GFP__mRNA"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> GFP__phi </ci>
              <ci> GFP__mRNA </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="GFP___J2" reversible="false">
        <listOfReactants>
          <speciesReference species="GFP__mRNA" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="GFP__mRNA" constant="true"/>
          <speciesReference species="GFP__protein" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="GFP__mRNA"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> GFP__k </ci>
              <ci> GFP__mRNA </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="GFP___J3" reversible="false">
        <listOfReactants>
          <speciesReference species="GFP__protein" constant="true"/>
        </listOfReactants>
        <listOfModifiers>
          <modifierSpeciesReference species="GFP__protein"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> GFP__d </ci>
              <ci> GFP__protein </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
