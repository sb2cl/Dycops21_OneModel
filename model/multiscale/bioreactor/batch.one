import './bioreactor.one'

model Batch(Bioreactor)
  # Model of a bioreactor in batch mode.

  rule
    F_in  := 0 # Set input media flux to zero.
    F_out := 0 # Set output waste flux to zero.
  end

end

standalone
  parameter __t_end = 1000
  
  parameter
    # Average cell mass.
    m_p = 433         # fg
    
    # Half activation threshold of growth rate.
    K_s = 0.1802      # g/L
  end
  
  bio = Batch()
  
  rule 
    bio.m_p := m_p
    bio.mu := log(2)/24*bio.s/(bio.s+K_s)
  end
end
